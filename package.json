{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:check": "prettier ./.prettierrc --check \"src/**/*{.ts,.js,.json,.scss,.css}\"",
    "format:fix": "prettier ./.prettierrc --write \"src/**/*{.ts,.js,.json,.scss,.css}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.7",
    "@angular/common": "~11.0.7",
    "@angular/compiler": "~11.0.7",
    "@angular/core": "~11.0.7",
    "@angular/forms": "~11.0.7",
    "@angular/platform-browser": "~11.0.7",
    "@angular/platform-browser-dynamic": "~11.0.7",
    "@angular/router": "~11.0.7",
    "husky": "^4.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "~11.0.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "browsercompat": "0.0.1",
    "browserslist": "^4.14.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check && npm run lint"
    }
  }
}
